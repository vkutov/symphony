{% extends 'base.html.twig' %}

{% block body %}
    <table border="1">
        <tr>
            <th>Make</th>
            <th>Model</th>
            <th>Travelled Distance</th>
        </tr>
        {% for car in cars %}
            <tr>
                <td>{{ car.make }}</td>
                <td>{{ car.model}}</td>
                <td> {{ car.travelledDistance }}</td>
                {#<td>{{ customer.isYoungDriver}}</td>#}
            </tr>
        {% endfor %}
    </table>
{% endblock %}

